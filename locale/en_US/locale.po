msgid "plugins.generic.piwik.displayName"
msgstr "Piwik Plugin"

msgid "plugins.generic.piwik.description"
msgstr "Integrate OJS with Piwik, a free and open source web site traffic analysis application. Requires that Piwik is already installed. Please see the <a href=\"http://piwik.org/\" title=\"piwik site\">Piwik site</a> for more information."

msgid "plugins.generic.piwik.manager.settings.description"
msgstr "<p>With this plugin enabled Piwik may be used to collect and analyze web site usage and traffic for this journal. Please note that this plugin requires that Piwik is already installed. Please see the <a href=\"http://piwik.org/\" title=\"Piwik site\">Piwik site</a> for more information.</p>"

msgid "plugins.generic.piwik.manager.settings.piwikUrl"
msgstr "Piwik Url"

msgid "plugins.generic.piwik.manager.settings.piwikUrlInstructions"
msgstr "The full url to your Piwik installation (i.e. http://myserver.net/piwik)."

msgid "plugins.generic.piwik.manager.settings.piwikUrlRequired"
msgstr "Please enter a valid url. Include http:// at the start."

msgid "plugins.generic.piwik.manager.settings.piwikSiteId"
msgstr "Site profile ID"

msgid "plugins.generic.piwik.manager.settings.piwikSiteIdInstructions"
msgstr "Within Piwik, click on Settings -> Websites. The site profile ID is displayed next to your site profile name."

msgid "plugins.generic.piwik.manager.settings.piwikSiteIdRequired"
msgstr "Please enter a site profile ID."
